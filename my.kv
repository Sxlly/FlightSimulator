<MyLayout>

    num_aircrafts: num_aircrafts
    filename: filename

    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height
        padding: 10
        spacing: 10


        Label:
            text: "Number Of Aircraft"
        
        TextInput:
            multiline: False
            id: num_aircrafts
        
        Label:
            text: "Height File Name"
        
        TextInput:
            multiline: False
            id: filename

        Button:
            text: "Enter"
        
        Button:
            text: "Clear"

